nome = input("Digite seu nome: ")
email = input("Digite seu email: ")
idade = input("Digite sua idade : ")
profissao = input("Digite sua profissao: ")
hobby = input("Digite seu hobby: ")

print(f"\nolá, {nome}! Bem-vindo.")
print(f"sua profissao atualmente é {profissao}.")
print(f"sua idade é {idade}.")
print(f"seu email cadastrado é {email}!")
print(f"Nas horas vagas, voce gosta de {hobby}!")
